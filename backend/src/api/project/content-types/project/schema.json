{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "start_date": {
      "type": "date"
    },
    "release_date": {
      "type": "date"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": ["images", "videos"]
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": ["images", "videos"]
    },
    "technologies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::technology.technology",
      "inversedBy": "projects"
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "deploy_url": {
      "type": "string"
    },
    "download_url": {
      "type": "string"
    },
    "github_url": {
      "type": "string"
    },
    "project_status": {
      "type": "enumeration",
      "enum": [
        "completed",
        "ongoing",
        "private"
      ],
      "default": "completed"
    },
    "access_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": ["images"]
    }
  }
}
